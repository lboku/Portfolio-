    {
      "name": "add new user",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Georgian phone numbers format check\", function () {",
              "    var jsonData = pm.response.json();",
              "    jsonData.users?.forEach(function(user) {",
              "        if(user.phone.startsWith(\"+995\")) {",
              "            pm.expect(user.phone).to.match(/^\\+995\\d{9}$/);",
              "        }",
              "    });",
              "});",
              "",
              "const expectedFirstName = pm.variables.get(\"firstName\");",
              "const expectedLastName = pm.variables.get(\"lastName\");",
              "const expectedFullName = pm.variables.get(\"fullName\");",
              "",
              "",
              "const responseData = pm.response.json();",
              "",
              "pm.test(\"სრული სახელი (name) სწორად არის გენერირებული\", function () {",
              "    const actualName = responseData.name;",
              "    pm.expect(actualName).to.contain(expectedFirstName);",
              "    pm.expect(actualName).to.contain(expectedLastName);",
              "});",
              "",
              "",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "const first = pm.variables.replaceIn(\"{{$randomFirstName}}\");",
              "const last  = pm.variables.replaceIn(\"{{$randomLastName}}\");",
              "",
              "pm.variables.set(\"firstName\", first);",
              "pm.variables.set(\"lastName\", last);",
              "pm.variables.set(\"fullName\", `${first} ${last}`);",
              "",
              "",
              "const phone = \"+9955\" + Math.floor(10000000 + Math.random() * 90000000);",
              "pm.variables.set(\"phone\", String(phone));",
              "",
              "const addemail =`${first}${last}@gmail.com`",
              "pm.variables.set(\"email\", addemail)",
              "",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
