    {
      "name": "change all value",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "const first = pm.variables.replaceIn(\"{{$randomFirstName}}\");",
              "const last  = pm.variables.replaceIn(\"{{$randomLastName}}\");",
              "",
              "pm.variables.set(\"firstName\", first);",
              "pm.variables.set(\"lastName\", last);",
              "pm.variables.set(\"fullName\", `${first} ${last}`);",
              "pm.variables.set(\"randomEmail\", `${first}${last}@gmail.com`);",
              "",
              "const phone = \"+9955\" + Math.floor(10000000 + Math.random() * 90000000);",
              "pm.variables.set(\"phone\", String(phone));"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "const responseData = pm.response.json();",
              "const expectedFirstName = pm.variables.get(\"firstName\");",
              "const expectedLastName = pm.variables.get(\"lastName\");",
              "const expectedFullName = pm.variables.get(\"fullName\");",
              "const expectedEmail = pm.variables.get(\"randomEmail\");",
              "const expectedPhone = pm.variables.get(\"phone\");",
              "",
              "pm.test(\"სტატუს კოდი არის 200 - მონაცემები წარმატებით განახლდა\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"FirstName წარმატებით შეიცვალა\", function () {",
              "    pm.expect(responseData.firstName).to.eql(expectedFirstName);",
              "});",
              "",
              "pm.test(\"LastName წარმატებით შეიცვალა\", function () {",
              "    pm.expect(responseData.lastName).to.eql(expectedLastName);",
              "});",
              "",
              "pm.test(\"FullName (სახელი + გვარი) ვალიდურია\", function () {",
              "    pm.expect(responseData.name).to.eql(expectedFullName);",
              "});",
              "",
              "pm.test(\"Email წარმატებით განახლდა\", function () {",
              "    pm.expect(responseData.email).to.eql(expectedEmail);",
              "});",
              "",
              "pm.test(\"Phone ნომერი სწორად განახლდა\", function () {",
              "    pm.expect(responseData.phone).to.eql(expectedPhone);",
              "});",
              "",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
